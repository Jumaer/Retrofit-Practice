<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffff"
    tools:context=".MainNext2">
    <androidx.recyclerview.widget.RecyclerView
        android:background="#ffff"
        android:id="@+id/recycleView_Display_data"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-115dp"
        tools:layout_editor_absoluteY="-1dp">


    </androidx.recyclerview.widget.RecyclerView>

    <Button
        android:id="@+id/nextApi2"
        android:layout_width="106dp"
        android:layout_height="50dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:layout_marginBottom="32dp"
        android:padding="4dp"
        android:text="Next API display2"
        android:textColor="#090808"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.947"
        app:layout_constraintStart_toStartOf="@+id/recycleView_Display_articles"
        app:layout_constraintTop_toTopOf="@+id/recycleView_Display_articles"
        app:layout_constraintVertical_bias="0.953"
        tools:ignore="OnClick" />

</androidx.constraintlayout.widget.ConstraintLayout>